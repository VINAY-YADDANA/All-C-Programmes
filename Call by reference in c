
#include <stdio.h>

int main()
{
   int a=10;
   int b=20;
   printf("Before swap a b are: %d %d\n",a,b);
   swap(&a,&b);
   printf("After swap a b are: \t%d \t%d\n",a,b);
   return 0;
}
void swap(int* x,int* y)
{
    int temp;
    temp=*x;
    *x=*y;
    *y=temp;
    printf("\nAfter swap x y are: \t%d \t%d",*x,*y);
}
